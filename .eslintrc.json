{
	"root": true,
	"extends": [
		"wikimedia/client",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"globals": {
		"collabpad": "readonly",
		"collabpads": "readonly",
		"jQuery": "readonly",
		"mediaWiki": "readonly",
		"mws": "readonly",
		"OOJSPlus": "readonly",
		"ve": "readonly",
		"bs": "readonly"
	},
	"rules": {
		"camelcase": "off",
		"no-jquery/no-done-fail": "warn"
	},
	"parserOptions": {
		"ecmaVersion": 8
	}
}
